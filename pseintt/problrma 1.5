Proceso sin_titulo
	Definir estatura como entero;
	Escribir "¿cual es su estatura?";
	Leer estatura;
	si estatura>164 Entonces
		Escribir "tu estatura es la indicada para subir a la montaña rusa";
	FinSi
	si estatura<165 Entonces
		Escribir "no puedes pasar, no cumples con la estatura requerida";
	FinSi
	Escribir "que pase el siguiente por favor";
FinProceso
