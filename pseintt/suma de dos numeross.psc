Proceso sin_titulo
	Definir n1, n2, res Como Real;
	Escribir 'escribe el primer numero';
	Leer n1;
	Escribir 'escribe el segundo numero';
	Leer n2;
	res <- n1+n2;
	Escribir 'la suma de dos numeros es :', res;
	Si res>=30 Entonces
		Escribir 'tu numero es igual o mayor a 30';
	FinSi
FinProceso
